{
  "kind": "Pod",
  "apiVersion": "v1",
  "metadata": {
    "name": "nginx-pinger",
    "labels": {
      "name": "nginx-pinger"
    }
  },
  "spec": {
    "containers": [
      {
        "name": "nginx",
        "image": "nginx",
        "ports": [
          {
            "hostPort": 85,
            "containerPort": 80
          }
        ]
      },
      {
        "name": "pinger",
        "image": "reselbob/pinger",
        "ports": [
          {
            "hostPort": 3000,
            "containerPort": 3000
          }
        ]
      }
    ]
  }
}